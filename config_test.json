{
    "output_folder":"input/my_experiment",
    "device" : "cuda",
    "seed" : "1917",  
    "mix_precision" : "False",

    "data_args":{
        "metadata_path" : "input/birdclef-2024/train_metadata.csv",
        "holdout_args":{
            "use_holdout" : true,
            "load_holdout" : false,
            "loading_holdout_path" : "input/my_experiment/holdout_182bottom_90.0%_ratleq5_.csv",
            "k":182,
            "percent":0.9,
            "rating_leq_than":5
        },
        "trainval_split_args":{
            "type": "stratifiedkfold",
            "n_folds": 3,
            "shuffle":true,
            "seed":1917
        }        
    },
    
    "preprocess_train_args":{
        "run_preprocess":true,
        "loading_trainval_feats_path" : "input/my_experiment/trainval_feats",
        "output_feature_types": ["audio", "spectrogram", "image"],
        "sampling_rate":32000,
        "audio_window":"center",
        "spec_n_fft" : 1095,
        "spec_win_len" : 412,
        "spec_hop_len" : 100,
        "spec_win_type": "hann",
        "spec_cut_min_freq" : 40,
        "spec_cut_max_freq" : 15000,
        "img_size":256
    },

    "data_augmentation_args":{
        "USE_HORIZFLIP":true,
        "USE_XYMASKING":true
    },

    "model_args":{
        "model" : "efficientnet_b0"
    },

    "training_args":{
        "features": "images",
        "batch_size" : 32,
        "n_workers" : 4,
        "epochs" : 5,
        "lr" : 1e-3,
        "weight_decay" : 1e-5
    }
    
}
